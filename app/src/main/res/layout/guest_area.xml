        <LinearLayout
            android:id="@+id/facebook_feeds"
            grid:layout_row="4"
            grid:layout_column="0"
            grid:layout_columnWeight="1"
            grid:layout_rowWeight="1"
            grid:layout_columnSpan="1"
            grid:layout_rowSpan="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/ebro"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <ImageView
                android:id="@+id/three"
                android:layout_width="55dp"
                android:layout_height="0dp"
                android:src="@drawable/ebrochure"
                android:layout_gravity="center"
                android:layout_weight="0.85"/>

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/fb"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/three"
                android:text="@string/edocs"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:textAlignment="gravity"
                android:paddingEnd="3dp"
                android:maxLength="14"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_weight="0.15"
                android:paddingRight="3dp"
                android:textSize="@dimen/dimen_normal_callibri"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/siteprogress"
            android:layout_margin="3dp"
            android:background="@drawable/sitep"
            android:clickable="true"
            android:orientation="vertical"
            android:weightSum="1"
            grid:layout_column="1"
            grid:layout_columnSpan="2"
            grid:layout_columnWeight="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            grid:layout_row="3"
            grid:layout_rowSpan="1"
            grid:layout_rowWeight="1">

            <com.nebulacompanies.nebula.view.MyTextView
                android:layout_marginTop="15dp"
                android:layout_width="25dp"
                android:layout_height="0dp"
                android:layout_weight="0.15"/>

            <ImageView
                android:id="@+id/twelve"
                android:layout_width="45dp"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="0.7"
                android:src="@drawable/site_progress" />

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/site"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:paddingEnd="3dp"
                android:maxLength="13"
                android:ellipsize="end"
                android:maxLines="1"
                android:paddingRight="3dp"
                android:text="@string/site_progress"
                android:textAlignment="gravity"
                android:textColor="@android:color/white"
                android:layout_weight="0.15"
                android:textSize="@dimen/dimen_normal_callibri"
                tools:ignore="ObsoleteLayoutParam" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/contact_us"
            grid:layout_row="4"
            grid:layout_column="1"
            grid:layout_columnWeight="1"
            grid:layout_rowWeight="1"
            grid:layout_columnSpan="1"
            grid:layout_rowSpan="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/cont"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <com.nebulacompanies.nebula.view.MyTextView
                android:layout_marginTop="15dp"
                android:layout_width="25dp"
                android:layout_height="0dp"
                android:layout_weight="0.15"/>

            <ImageView
                android:id="@+id/nine"
                android:layout_width="45dp"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="0.7"
                android:src="@drawable/contact_us_1" />

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/cu"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/nine"
                android:text="@string/contact_us"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:textAlignment="gravity"
                android:paddingEnd="3dp"
                android:maxLength="16"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_weight="0.15"
                android:paddingRight="3dp"
                android:textSize="@dimen/dimen_normal_callibri"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/notifications"
            grid:layout_row="4"
            grid:layout_column="2"
            grid:layout_columnWeight="1"
            grid:layout_rowWeight="1"
            grid:layout_rowSpan="1"
            grid:layout_columnSpan="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/notification"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/badge"
                android:background="@drawable/badge_circle"
                android:layout_width="25dp"
                android:layout_height="0dp"
                android:layout_weight="0.15"
                android:textStyle="bold"
                android:textAlignment="center"
                android:textColor="#faf7f7"
                android:gravity="center"
                android:layout_gravity="end"
                android:layout_alignParentTop="true"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="15dp"
                android:visibility="invisible"
                android:layout_marginRight="15dp"
                android:textSize="@dimen/dimen_normal_callibri"/>

            <ImageView
                android:id="@+id/ten"
                android:layout_width="45dp"
                android:layout_height="0dp"
                android:layout_weight="0.7"
                android:src="@drawable/notifications_1"
                android:layout_gravity="center_horizontal"
                android:layout_below="@+id/textView"
                android:layout_centerHorizontal="true"/>

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/noti"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/ten"
                android:text="@string/notifications"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:paddingEnd="3dp"
                android:maxLength="13"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_weight="0.15"
                android:paddingRight="3dp"
                android:textSize="@dimen/dimen_normal_callibri"/>
        </LinearLayout>
    </androidx.gridlayout.widget.GridLayout>
</ScrollView>