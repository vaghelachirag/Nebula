            <LinearLayout
                android:id="@+id/customer_booking_layout"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal"
                android:background="@drawable/cust_login_drawable"
                android:clickable="true">

                <ImageView
                    android:id="@+id/cust_icon"
                    android:layout_width="48dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_account_circle_white_24dp" />

                <com.nebulacompanies.nebula.view.MyTextView
                    android:id="@+id/cust_text"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:text="@string/cust_login"
                    android:textColor="@android:color/white"
                    android:layout_gravity="center"
                    android:paddingEnd="3dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:maxLength="14"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:layout_marginStart="10dp"
                    android:paddingRight="3dp"
                    android:layout_marginLeft="10dp"
                    android:textSize="@dimen/dimen_normal_callibri"/>

            </LinearLayout>

        </LinearLayout>

