<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:grid="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fillViewport="true"
    android:background="@android:color/black">

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        grid:columnCount="3"
        grid:rowCount="5"
        android:background="@android:color/black">

        <LinearLayout
            android:id="@+id/company_profile"
            grid:layout_row="0"
            grid:layout_column="0"
            grid:layout_columnWeight="1"
            grid:layout_rowWeight="4"
            grid:layout_columnSpan="1"
            grid:layout_rowSpan="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/comp_pro"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <ImageView
                android:id="@+id/one"
                android:layout_width="45dp"
                android:layout_height="0dp"
                android:src="@drawable/company_profile_1"
                android:layout_gravity="center"
                android:layout_weight="0.85"/>

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/com"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/one"
                android:text="@string/company_profile"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:maxLength="15"
                android:ellipsize="end"
                android:maxLines="1"

                android:textAlignment="gravity"
                android:paddingEnd="3dp"
                android:textSize="@dimen/dimen_normal_callibri"
                android:layout_weight="0.15"
                android:paddingRight="3dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/products_"
            grid:layout_row="0"
            grid:layout_column="1"
            grid:layout_columnWeight="1"
            grid:layout_rowWeight="1"
            grid:layout_columnSpan="1"
            grid:layout_rowSpan="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/prodct"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <ImageView
                android:id="@+id/two"
                android:layout_width="45dp"
                android:layout_height="0dp"
                android:src="@drawable/products_1"
                android:layout_gravity="center"
                android:layout_weight="0.85"/>

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/pro"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/two"
                android:layout_gravity="center"
                android:layout_weight="0.15"
                android:paddingEnd="3dp"
                android:text="@string/projects_"
                android:maxLength="11"
                android:ellipsize="end"
                android:maxLines="1"
                android:textAlignment="gravity"
                android:textColor="@android:color/white"
                android:paddingRight="3dp"
                android:textSize="@dimen/dimen_normal_callibri"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/multi_langs"
            grid:layout_row="0"
            grid:layout_column="2"
            grid:layout_columnWeight="1"
            grid:layout_rowWeight="1"
            grid:layout_columnSpan="1"
            grid:layout_rowSpan="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/lang"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <ImageView
                android:id="@+id/three_new"
                android:layout_width="55dp"
                android:layout_height="0dp"
                android:src="@drawable/language"
                android:layout_gravity="center"
                android:layout_weight="0.85"/>

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/fb_new"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/three_new"
                android:text="@string/language"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:textAlignment="gravity"
                android:paddingEnd="3dp"
                android:maxLength="8"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_weight="0.15"
                android:paddingRight="3dp"
                android:textSize="@dimen/dimen_normal_callibri"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/events"
            grid:layout_row="1"
            grid:layout_column="0"
            grid:layout_columnWeight="1"
            grid:layout_columnSpan="1"
            grid:layout_rowSpan="1"
            grid:layout_rowWeight="4"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/event"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <ImageView
                android:id="@+id/four"
                android:layout_width="45dp"
                android:layout_height="0dp"
                android:src="@drawable/events_1"
                android:layout_gravity="center"
                android:layout_weight="0.85"/>

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/eve"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/four"
                android:text="@string/events"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:textAlignment="gravity"
                android:paddingEnd="3dp"
                android:maxLength="6"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_weight="0.15"
                android:paddingRight="3dp"
                android:textSize="@dimen/dimen_normal_callibri"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/videos"
            grid:layout_row="1"
            grid:layout_column="1"
            grid:layout_columnWeight="1"
            grid:layout_rowWeight="1"
            grid:layout_columnSpan="2"
            grid:layout_rowSpan="1"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            android:background="@drawable/video"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_margin="3dp"
            android:clickable="true">

            <ImageView
                android:id="@+id/five"
                android:layout_width="45dp"
                android:layout_height="0dp"
                android:src="@drawable/videos_1"
                android:layout_gravity="center"
                android:layout_weight="0.85"/>

            <com.nebulacompanies.nebula.view.MyTextView
                android:id="@+id/vid"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_below="@id/five"
                android:text="@string/videos"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:textAlignment="gravity"
                android:paddingEnd="3dp"
                android:maxLength="6"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_weight="0.15"
                android:paddingRight="3dp"
                android:textSize="@dimen/dimen_normal_callibri"/>

        </LinearLayout>



        <LinearLayout
            android:id="@+id/nebula_logo"

            android:layout_height="87dp"
            android:layout_margin="3dp"
            android:background="@drawable/logo"
            android:clickable="true"
            android:orientation="vertical"
            android:weightSum="1"
            grid:layout_column="0"
            grid:layout_columnSpan="1"
            grid:layout_columnWeight="2"
            grid:layout_gravity="fill_horizontal|fill_vertical"
            grid:layout_row="3"
            grid:layout_rowSpan="1"
            grid:layout_rowWeight="1">

            <ImageView
                android:id="@+id/six"
                android:layout_width="90dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:src="@drawable/nebula_logo" />

        </LinearLayout>

